{
  "compilerOptions": {
      "moduleResolution": "node",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "target": "es3",
      "outDir": "js",
      "sourceMap": false
  },
  "include": [
      "ts/**/*", "tests/mock"
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}
